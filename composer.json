{
    "name": "symfonycorp/connect-bundle",
    "description": "Official bundle for the SymfonyConnect SDK",
    "type": "symfony-bundle",
    "keywords": ["symfonycorp connect"],
    "homepage": "https://github.com/symfonycorp/connect-bundle",
    "license": "MIT",
    "authors": [],
    "require": {
        "php": ">=7.1.3",
        "symfonycorp/connect": "^6.1"
    },
    "require-dev": {
        "symfony/security-bundle": "^4.4|^5.0",
        "symfony/phpunit-bridge": "^4.4|^5.0"
    },
    "conflict": {
        "symfony/config": "<4.2"
    },
    "autoload": {
        "psr-4": {
            "SymfonyCorp\\Bundle\\ConnectBundle\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.x-dev"
        }
    }
}
